{
	"compilerOptions": {
		"target": "ES2017",
		"lib": ["ES2017", "DOM"],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"declaration": false,
		"sourceMap": true,
		"skipLibCheck": true,
		"noUncheckedIndexedAccess": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src", "test", "vite.config.ts"]
}
